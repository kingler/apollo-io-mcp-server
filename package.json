{
  "name": "apollo-io-mcp-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:worker": "tsc -p tsconfig.worker.json",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:worker": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=integration,e2e",
    "test:unit:quick": "jest --testPathIgnorePatterns=integration,e2e --maxWorkers=1 --silent",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts tests/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "MCP server for Apollo.io sales intelligence and lead generation platform integration",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@prisma/client": "^6.1.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/express": "^5.0.3",
    "express": "^5.1.0",
    "itty-router": "^5.0.22",
    "prisma": "^6.1.0",
    "typescript": "^5.9.2",
    "uuid": "^11.0.3",
    "winston": "^3.15.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250823.0",
    "@eslint/js": "^9.34.0",
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.42.0",
    "wrangler": "^4.32.0"
  }
}
